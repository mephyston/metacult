{
    "name": "website",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/website/src",
    "targets": {
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "bun run dev",
                "cwd": "apps/website"
            }
        },
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "bun run build",
                "cwd": "apps/website"
            }
        }
    }
}